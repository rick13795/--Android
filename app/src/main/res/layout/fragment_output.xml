<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/fkc1"
    android:background="@mipmap/cat_beijing">
    <RelativeLayout
        android:orientation="vertical" android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/fragment_input"
        android:layout_marginTop="10dp">

        <ImageButton
            android:id="@+id/output_hongbao"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:layout_marginLeft="120dp"
            android:background="@drawable/add_biankuang2"
            android:src="@mipmap/output_hongbao">
        </ImageButton>
        <ImageButton
            android:id="@+id/output_zujing"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:layout_marginLeft="230dp"
            android:background="@drawable/add_biankuang2"
            android:src="@mipmap/output_zujin">
        </ImageButton>
        <ImageButton
            android:id="@+id/output_lijing"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:layout_alignParentRight="true"
            android:layout_marginRight="20dp"
            android:background="@drawable/add_biankuang2"
            android:src="@mipmap/output_lijin">
        </ImageButton>
        <ImageButton
            android:id="@+id/output_licai"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:layout_marginLeft="120dp"
            android:layout_marginTop="70dp"
            android:background="@drawable/add_biankuang2"
            android:src="@mipmap/output_licai">
        </ImageButton>
        <ImageButton
            android:id="@+id/output_nianzhongjiang"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:layout_marginLeft="230dp"
            android:layout_marginTop="70dp"
            android:background="@drawable/add_biankuang2"
            android:src="@mipmap/output_nianzhongjiang">
        </ImageButton>
        <ImageButton
            android:id="@+id/output_fenhong"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="70dp"
            android:background="@drawable/add_biankuang2"
            android:src="@mipmap/output_fenhong">
        </ImageButton>
        <ImageButton
            android:id="@+id/output_qita"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:layout_alignParentRight="true"
            android:layout_marginRight="20dp"
            android:layout_marginTop="70dp"
            android:background="@drawable/add_biankuang2"
            android:src="@mipmap/output_qita">
        </ImageButton>
        <ImageButton
            android:id="@+id/output_jieru"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="150dp"
            android:background="@drawable/add_biankuang2"
            android:src="@mipmap/output_jieru">
        </ImageButton>
        <ImageButton
            android:id="@+id/output_shoukuan"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:layout_marginLeft="120dp"
            android:layout_marginTop="150dp"
            android:background="@drawable/add_biankuang2"
            android:src="@mipmap/output_shoukuan">
        </ImageButton>

        <ImageButton
            android:id="@+id/output_gongzi"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:layout_marginLeft="20dp"
            android:background="@drawable/add_biankuang2"
            android:src="@mipmap/mingxi_gongzi"></ImageButton>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="350dp"
            android:layout_marginTop="230dp"
            android:background="#F7D878">

            <EditText
                android:id="@+id/wancheng"
                android:layout_width="170dp"
                android:layout_height="40dp"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="10dp"
                android:background="@drawable/add_biankuang"
                android:hint="点击写备注"
                android:singleLine="true"></EditText>

            <TextView
                android:id="@+id/number_input"
                android:layout_width="170dp"
                android:layout_height="40dp"
                android:layout_marginLeft="200dp"
                android:layout_marginTop="10dp"
                android:background="@drawable/add_biankuang"
                android:gravity="center_vertical"
                android:singleLine="true"></TextView>

            <Button
                android:id="@+id/numberout_1"
                android:layout_width="80dp"
                android:layout_height="50dp"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="60dp"
                android:background="@drawable/add_biankuang"
                android:text="1"></Button>

            <Button
                android:id="@+id/numberout_2"
                android:layout_width="80dp"
                android:layout_height="50dp"
                android:layout_marginLeft="110dp"
                android:layout_marginTop="60dp"
                android:background="@drawable/add_biankuang"
                android:text="2"></Button>

            <Button
                android:id="@+id/numberout_3"
                android:layout_width="80dp"
                android:layout_height="50dp"
                android:layout_marginLeft="200dp"
                android:layout_marginTop="60dp"
                android:background="@drawable/add_biankuang"
                android:text="3"></Button>

            <ImageButton
                android:id="@+id/rili"
                android:layout_width="80dp"
                android:layout_height="50dp"
                android:layout_marginLeft="290dp"
                android:layout_marginTop="60dp"
                android:background="@drawable/add_biankuang"
                android:src="@mipmap/mingxi_rili"></ImageButton>

            <Button
                android:id="@+id/numberout_4"
                android:layout_width="80dp"
                android:layout_height="50dp"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="120dp"
                android:background="@drawable/add_biankuang"
                android:text="4"></Button>

            <Button
                android:id="@+id/numberout_5"
                android:layout_width="80dp"
                android:layout_height="50dp"
                android:layout_marginLeft="110dp"
                android:layout_marginTop="120dp"
                android:background="@drawable/add_biankuang"
                android:text="5"></Button>

            <Button
                android:id="@+id/numberout_6"
                android:layout_width="80dp"
                android:layout_height="50dp"
                android:layout_marginLeft="200dp"
                android:layout_marginTop="120dp"
                android:background="@drawable/add_biankuang"
                android:text="6"></Button>

            <ImageButton
                android:layout_width="80dp"
                android:layout_height="50dp"
                android:layout_marginLeft="290dp"
                android:layout_marginTop="120dp"
                android:background="@drawable/add_biankuang"
                android:src="@mipmap/add_add"></ImageButton>

            <Button
                android:id="@+id/numberout_7"
                android:layout_width="80dp"
                android:layout_height="50dp"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="180dp"
                android:background="@drawable/add_biankuang"
                android:text="7"></Button>

            <Button
                android:id="@+id/numberout_8"
                android:layout_width="80dp"
                android:layout_height="50dp"
                android:layout_marginLeft="110dp"
                android:layout_marginTop="180dp"
                android:background="@drawable/add_biankuang"
                android:text="8"></Button>

            <Button
                android:id="@+id/numberout_9"
                android:layout_width="80dp"
                android:layout_height="50dp"
                android:layout_marginLeft="200dp"
                android:layout_marginTop="180dp"
                android:background="@drawable/add_biankuang"
                android:text="9"></Button>

            <ImageButton
                android:layout_width="80dp"
                android:layout_height="50dp"
                android:layout_marginLeft="290dp"
                android:layout_marginTop="180dp"
                android:background="@drawable/add_biankuang"
                android:src="@mipmap/add_delete1"></ImageButton>

            <Button
                android:id="@+id/finist_out"
                android:layout_width="80dp"
                android:layout_height="50dp"
                android:layout_marginLeft="290dp"
                android:layout_marginTop="240dp"
                android:background="@drawable/add_biankuang1"
                android:text="完成"
                android:textColor="#ffffff"></Button>

            <ImageButton
                android:layout_width="80dp"
                android:layout_height="50dp"
                android:layout_marginLeft="200dp"
                android:layout_marginTop="240dp"
                android:background="@drawable/add_biankuang"
                android:src="@mipmap/add_delete"></ImageButton>

            <Button
                android:id="@+id/numberout_0"
                android:layout_width="80dp"
                android:layout_height="50dp"
                android:layout_marginLeft="110dp"
                android:layout_marginTop="240dp"
                android:background="@drawable/add_biankuang"
                android:text="0"></Button>

            <Button
                android:id="@+id/deleteout"
                android:layout_width="80dp"
                android:layout_height="50dp"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="240dp"
                android:background="@drawable/add_biankuang"
                android:text="清除"></Button>

        </RelativeLayout>
    </RelativeLayout>
</LinearLayout>